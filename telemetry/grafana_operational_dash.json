          "pluginVersion": "4.11.2",
          "queryType": "timeseries",
          "rawSql": "WITH trades AS (\n  SELECT\n    Timestamp,\n    SpanId,\n    SpanAttributes['market']                 AS market,\n    lowerUTF8(SpanAttributes['side'])        AS side,\n    toFloat64OrNull(SpanAttributes['size'])  AS size,\n    toFloat64OrNull(SpanAttributes['price']) AS price\n  FROM default.otel_traces\n  WHERE $__timeFilter(Timestamp)\n    AND SpanName = 'process_user_datum'\n    AND SpanAttributes['event_type'] = 'trade'\n),\npriced AS (\n  SELECT\n    trades.*,\n    sumIf(size,       side = 'buy')  OVER w_prev AS buy_qty_prev,\n    sumIf(size*price, side = 'buy')  OVER w_prev AS buy_val_prev,\n    ifNull(buy_val_prev / nullIf(buy_qty_prev, 0), 0) AS avg_cost_prev\n  FROM trades\n  WINDOW w_prev AS (\n    PARTITION BY market\n    ORDER BY Timestamp, SpanId\n    ROWS BETWEEN UNBOUNDED PRECEDING AND 1 PRECEDING\n  )\n)\nSELECT\n  toDateTime(intDiv(toUInt32(Timestamp), $__interval_s) * $__interval_s) AS time,\n  market,\n  sumIf(size * (price - avg_cost_prev), side = 'sell') AS realized_pnl\nFROM priced\nGROUP BY time, market\nORDER BY time, market;\n",
          "refId": "A"
        },
